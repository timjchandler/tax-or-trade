<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="150.0" prefWidth="300.0" style="-fx-background-color: #1d1d1d; -fx-border-color: #353535; -fx-border-width: 5px;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.SeedController">
   <children>
      <Button layoutX="15.0" layoutY="104.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="170.0" AnchorPane.topAnchor="100.0" />
      <Button layoutX="170.0" layoutY="104.0" mnemonicParsing="false" onAction="#setSeed" text="Set Seed" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="170.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="100.0" />
      <TextField fx:id="valueBox" layoutX="170.0" layoutY="14.0" AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="170.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="15.0" />
      <Text fill="#eee3e3" layoutX="32.0" layoutY="39.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter a seed value:">
         <font>
            <Font size="15.0" />
         </font>
      </Text>
      <Text fx:id="errorPrompt" fill="#dbbd6b" layoutX="72.0" layoutY="80.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please enter an integer value" />
   </children>
</AnchorPane>
